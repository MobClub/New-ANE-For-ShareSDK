        <packagedDependency>SDK/ShareSDK/Support/PlatformSDK/FacebookMessengerSDK/FBSDKMessengerShareKit.framework</packagedDependency>
        <packagedDependency>SDK/ShareSDK/Support/PlatformSDK/FacebookMessengerSDK/FBSDKCoreKit.framework</packagedDependency>
        <packagedDependency>SDK/ShareSDK/Support/PlatformSDK/FacebookMessengerSDK/Bolts.framework</packagedDependency>
        <packagedDependency>SDK/ShareSDK/Support/PlatformSDK/RenRenSDK/RennSDK.framework</packagedDependency>
        <packagedDependency>SDK/ShareSDK/Support/PlatformSDK/LineSDK/LineSDK.framework</packagedDependency>
    </packagedDependencies>
</platform>
